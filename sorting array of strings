
#include <stdio.h>
#include<string.h>

int main()
{
    char *a[4]={"hello","hi","ball","apple"};
    char *temp;
    int i,j,val,k;
    
    for(i=0;i<4;i++)
    {
        for(j=j;j<4;j++)
        {
            val=strcmp(a[i],a[j]);
            if(val>0)
            {
                temp = a[i];
                a[i]=a[j];
                a[j]=temp;
            }
            

        }

    }


    for(i=0;i<4;i++)
    {
        printf("%s\t",a[i]);
    }
    return 0;
}
