#include<stdio.h>

int main()
{
    int a[3][3]={{1,2,3},{2,3,4},{3,4,5}};
    int *p=NULL;
    
    p=&a[0][0];
    for(int i=0;i<3;i++)
    {
        for(int j=0;j<3;j++)
        {
           // printf("a[%d][%d]= %d\t",i,j, *(p+(i*column)+j));
            //Read element of 2D array
            printf("aiData[%d][%d] = %d\n",i,j, *(p + ( i * 3) + j));
        }
        printf("\n");
    }
    return 0;
}
