#include<stdio.h>

int main()
{
    int a[]={1,2,3,4,5};
    
    int size;
    size = sizeof(a)/sizeof(a[0]);
    printf("size %d\n",size);
    
    printf("a[0] %d\n",a[0]);
    printf("a %d\n",a); //printing base address negetive value
    printf("a %u\n",a); // printing base address
    printf("i[a] %d",1[a]);
    printf("*a %d\n",*a);
    printf("*(a+i)  %d\n",*(a+1));// value at a[1]
    printf(" a+i %d\n",(a+1)); // address of a[1]
    
    return 0;
}
