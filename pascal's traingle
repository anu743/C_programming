#include<stdio.h>

int main()
{
  int rows=5,i,j;
  int space,val;
  for(i=0;i<rows;i++)
  {
    for(space=1;space<=rows-i;space++)
    printf("  ");
    
    for(j=0;j<=i;j++)
    {
     if(j==0 | i==0)
     val =1;
     else
     val = val *(i-j+1)/j;
     
     printf("%4d",val);
    
    }
    printf("\n");
  
  }

  return 0;
 }
