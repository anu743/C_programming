#include<stdio.h>

int main()
{
  int rows=5, space,k,i;
  
   for(i=1;i<=rows;i++,k=0)
   {
       for(space=0;space<=rows-i;space++)
       {
           printf(" ");
       }
       
       while(k != (2*i-1))
       {
           printf("*");
           k++;
       }
       printf("\n");
   }
    
    return 0;
}
