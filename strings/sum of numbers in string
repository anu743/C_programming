#include<stdio.h>
#include<string.h>

int main()
{
    char *str="hello12345";
    int l=strlen(str);
    int i,sum=0;
    for(i=0;i<l;i++)
    {
        if((str[i]>='0')&&(str[i]<='9'))
        {
            sum=sum+(str[i]-'0');
        }
       // printf("inside for loop\n");
    }
    
    printf("%d",sum);
    return 0;
}
